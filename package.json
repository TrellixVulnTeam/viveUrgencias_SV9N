{
  "name": "viveurgencias",
  "version": "1.0.0",
  "description": "Borrador aplicaci√≥n vive urgencias",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "start": "npm-run-all --parallel dev watch:sass",
    "compile:sass": "node-sass public/sass/main.scss public/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' public/css/style.comp.css -o public/css/style.prefix.css",
    "compress:css": "node-sass public/css/style.prefix.css public/css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hybryjjrz/viveUrgencias.git"
  },
  "author": "JJ Ramz",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hybryjjrz/viveUrgencias/issues"
  },
  "homepage": "https://github.com/hybryjjrz/viveUrgencias#readme",
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.6",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
